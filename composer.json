{
    "name": "site/site-endpoints",
    "type": "typo3-cms-extension",
    "description": "Provides a middleware for registering Slim PHP applications within TYPO3 Frontend Sites",
    "homepage": "https://github.com/iammati/site-endpoints",
    "version": "0.1.3",
    "keywords": [
        "typo3",
        "api",
        "endpoints",
        "psr-15"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "b13 Development Team",
            "homepage": "https://b13.com"
        },
        {
            "name": "Mati",
            "email": "mati_01@icloud.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/b13/slimphp/issues"
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-core": "^11.5",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0.2"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "site_endpoints"
        }
    },
    "autoload": {
        "psr-4": {
            "Site\\SiteEndpoints\\": "Classes/"
        }
    }
}
